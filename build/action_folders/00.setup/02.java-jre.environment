#!/bin/bash

export JAVA_HOME=${JAVA_HOME:-/usr/lib/jvm/java-1.8-openjdk}

declare -a env_jre=(
    'JAVA_ALPINE_VERSION=${JAVA_ALPINE_VERSION:-8.131.11-r2}'
    'JAVA_VERSION=${JAVA_VERSION:-8u131}'
    'JAVA_HOME="${JAVA_HOME:-/usr/lib/jvm/java-1.8-openjdk/jre}"'
    'PATH="${PATH}:${JAVA_HOME}/bin"'
)

lib.updateRuntimeEnvironment "${env_jre[*]}" 
