# build Packages

source "/tmp${CBF['crf_env']}/java.properties"
source "/tmp${CBF['crf_lib']}/java.bashlib"


if [ "${CBF['os_id']}" = 'alpine' ]; then
    apk add --no-cache openjdk8-jre="$JAVA_ALPINE_VERSION"
fi

if [ "$JAVA_HOME" != "$( java.home )" ]; then
    echo "java_home:  $( java.home )"
    echo "JAVA_HOME:  $JAVA_HOME"
    exit 1
fi 
